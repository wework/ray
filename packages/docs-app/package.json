{
  "name": "ray-website-gatsby",
  "description": "Ray Design System website",
  "private": true,
  "version": "1.16.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wework/ray"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{scss,css,js,md}\"",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,md}\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs",
    "netlify:build": "gatsby build && cp -r static/* ../../.out/ && cp -r public/* ../../.out/"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@reach/router": "^1.2.1",
    "add": "2.0.6",
    "color": "^3.1.0",
    "custom-event": "^1.0.1",
    "dotenv": "^6.1.0",
    "gatsby": "^2.1.23",
    "gatsby-image": "2.0.20",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "2.0.4",
    "gatsby-plugin-sharp": "2.0.13",
    "gatsby-react-router-scroll": "^2.0.1",
    "gatsby-remark-autolink-headers": "^2.0.11",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-copy-linked-files": "2.0.6",
    "gatsby-remark-embed-video": "^1.4.0",
    "gatsby-remark-embedded-codesandbox": "^1.2.0",
    "gatsby-remark-images": "3.0.11",
    "gatsby-source-filesystem": "2.0.9",
    "gatsby-transformer-remark": "^2.3.1",
    "gatsby-transformer-sharp": "2.1.8",
    "markdown-it": "^8.4.2",
    "markdown-loader": "^4.0.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-ga": "^2.5.5",
    "react-helmet": "^5.2.0",
    "react-router": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-options": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@wework/ray-core": "file:../core",
    "babel-loader": "8.0.5",
    "cross-env": "^5.2.0",
    "fs-extra": "^7.0.1",
    "gatsby-remark-static-images": "^1.0.0",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.14",
    "node-sass": "^4.10.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0"
  },
  "eslintConfig": {
    "globals": {
      "__PATH_PREFIX__": true
    }
  }
}
